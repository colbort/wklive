// ===== auth =====
type LoginReq {
  username   string `json:"username"`
  password   string `json:"password"`
  googleCode string `json:"googleCode,optional"`
}

type LoginResp {
  code  int    `json:"code"`
  msg   string `json:"msg"`
  token string `json:"token"`
  exp   int64  `json:"exp"`
}

@server(
  prefix: /admin
  group: auth
)
service admin {
  @handler Login
  post /auth/login (LoginReq) returns (LoginResp)
}


